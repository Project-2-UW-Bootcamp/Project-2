<script src="profile.js"></script>

<html>
  <head>
    <title></title>
    <link href="//netdna.bootstrapcdn.com/bootstrap/3.1.1/css/bootstrap.min.css" rel="stylesheet"/>
  </head>
  <body>
    <div class="container">
      <div class="page-header">
        <h1>My Profile</h1>
      </div>
      <form class="login-form form-horizontal" method="post" role="form">
        <input name="_csrf" type="hidden"/>
        <div class="form-group">
          <label class="col-sm-4">First Name</label>
          <div class="col-sm-8">
            <input class="form-control" placeholder="Your first name" required="required" name="givenName" type="text"/>
          </div>
        </div>
        <div class="form-group">
          <label class="col-sm-4">Last Name</label>
          <div class="col-sm-8">
            <input class="form-control" placeholder="Your last name" required="required" name="surname" type="text"/>
          </div>
        </div>
        <div class="form-group">
          <label class="col-sm-4">Street address</label>
          <div class="col-sm-8">
            <input class="form-control" placeholder="e.g. 123 Sunny Ave" required="required" name="streetAddress" type="text"/>
          </div>
        </div>
        <div class="form-group">
          <label class="col-sm-4">City</label>
          <div class="col-sm-8">
            <input class="form-control" placeholder="e.g. City" required="required" name="city" type="text"/>
          </div>
        </div>
        <div class="form-group">
          <label class="col-sm-4">State</label>
          <div class="col-sm-8">
            <input class="form-control" placeholder="e.g. CA" required="required" name="state" type="text"/>
          </div>
        </div>
        <div class="form-group">
          <label class="col-sm-4">ZIP</label>
          <div class="col-sm-8">
            <input class="form-control" placeholder="e.g. 94116" required="required" name="zip" type="text"/>
          </div>
        </div>
        <div class="form-group">
          <div class="col-sm-offset-4 col-sm-8">
            <button class="login btn btn-primary" type="submit">Save</button>
          </div>
        </div>
        <div class="pull-right"><a href="/">Return to home page</a></div>
      </form>
    </div>
  </body>
</html>
  <script src="/js/profile.js"></script>